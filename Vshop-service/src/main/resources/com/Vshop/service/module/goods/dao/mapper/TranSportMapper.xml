<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.Vshop.service.module.goods.dao.mapper.TranSportMapper">
    <resultMap id="BaseResultMap" type="com.Vshop.core.entity.Transport">
        <result column="id" property="id" jdbcType="INTEGER" />
        <result column="title" property="title" jdbcType="VARCHAR" />
        <result column="send_tpl_id" property="sendTplId" jdbcType="INTEGER" />
    	<result column="store_id" property="storeId" jdbcType="INTEGER" />
    </resultMap>
    
   <sql id="Base_Column_List" >
        id, title, send_tpl_id, store_id
    </sql>

    <select id="queryStoreList" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select
        <include refid="Base_Column_List" />
        from shop_transport where store_id=#{storeId} and is_del=0
    </select>

    
</mapper>